<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="i3" class="com.abuzar.laptopdealer.i3"></bean>
    <bean id="i5" class="com.abuzar.laptopdealer.i5"></bean>
    <bean id="i7" class="com.abuzar.laptopdealer.i7"></bean>


    <bean id="dellWithi3" class="com.abuzar.laptopdealer.Dell">
        <constructor-arg ref="i3"></constructor-arg>
    </bean>

    <bean id="dellWithi5" class="com.abuzar.laptopdealer.Dell">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>

    <bean id="dellWithi7" class="com.abuzar.laptopdealer.Dell">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>



    <bean id="macbookWithi3" class="com.abuzar.laptopdealer.MacBook">
        <constructor-arg ref="i3"></constructor-arg>
    </bean>

    <bean id="macbookWithi5" class="com.abuzar.laptopdealer.MacBook">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>

    <bean id="macbookWithi7" class="com.abuzar.laptopdealer.MacBook">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>


    <bean id="lenovoWithi3" class="com.abuzar.laptopdealer.Lenovo">
        <constructor-arg ref="i3"></constructor-arg>
    </bean>

    <bean id="lenovoWithi5" class="com.abuzar.laptopdealer.Lenovo">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>

    <bean id="lenovoWithi7" class="com.abuzar.laptopdealer.Lenovo">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>

</beans>